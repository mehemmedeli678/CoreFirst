@{
}

<a href="/MyUser/Create" class="btn btn-primary">Create</a>
<table class="table">
    <thead>
        <tr>
            <th>Fullname</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Birtday</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var u in Model)
        {
        <tr>
            <td>@u.name</td>
            <td>@u.email</td>
            <td>@u.phonenumber</td>
            <td>@u.birthday</td>
            <td>
                <a href="/myuser/edit/@u.ID">Edit</a>|
                <a href="/myuser/delete/@u.ID">Delete</a>
            </td>
        </tr>
        }

    </tbody>
</table>